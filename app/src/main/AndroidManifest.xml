<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dicyapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/dicy_app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/dicy_app_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DICYAPP">
        <activity android:name=".DiceRoller_Settings"></activity>
        <activity android:name=".FourPlayerCounter" />
        <activity android:name=".ThreePlayerCounter" />
        <activity android:name=".TwoPlayerCounter" />
        <activity android:name=".LifeCounter" />
        <activity android:name=".NumberGenerator" />
        <activity android:name=".DiceRoller" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>